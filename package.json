{
  "name": "elf-boilerplate-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "serve:user": "cross-env VITE_PROJECT_TYPE=user vite",
    "serve:admin": "cross-env VITE_PROJECT_TYPE=admin vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue . --fix",
    "lint-check": "eslint --ext .js,.jsx,.ts,.tsx,.vue . --fix-dry-run",
    "ts-check": "vue-tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "lint-staged && vue-tsc --noEmit"
  },
  "lint-staged": {
    "*": "prettier --check",
    "*.{js,jsx,ts,tsx,vue}": "eslint"
  },
  "dependencies": {
    "@nestjsx/crud-request": "^5.0.0-alpha.3",
    "axios": "^0.26.0",
    "html2canvas": "^1.4.1",
    "jquery": "^3.7.0",
    "jspdf": "^2.5.1",
    "maska": "^1.5.0",
    "material-symbols": "^0.9.0",
    "msw": "^0.39.2",
    "pinia": "^2.0.11",
    "quill": "^1.3.7",
    "swiper": "^8.4.7",
    "uuid": "^8.3.2",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.25",
    "vue-currency-input": "^2.4.0",
    "vue-multiselect": "^3.0.0-alpha.2",
    "vue-router": "^4.0.12",
    "vue-toastification": "^2.0.0-rc.5",
    "waypoints": "^4.0.1",
    "xlsx": "^0.18.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@types/node": "^17.0.21",
    "@types/quill": "^2.0.9",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^8.0.3",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.6",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "sass": "^1.49.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-eslint": "^1.3.0",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.29.8"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
